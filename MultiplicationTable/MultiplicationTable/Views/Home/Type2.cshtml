
@{
    ViewData["Title"] = "Type2";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="table-responsive-lg py-5">
    <table class="table">
        <thead>
            <tr>
                @for (int i = 1; i <= 12; i++)
                {
                    <th scope="col">@i</th>
                }
            </tr>
        </thead>
        <tbody>
            @for (int i = 2; i <= 12; i++)
            {
                <tr>
                    @for (int j = 1; j <= 12; j++)
                    {
                        if (j == 1)
                        {
                            <th scope="row">@(j*i)</th>
                        }
                        else
                        {
                            <td scope="row">@(j*i)</td>
                        }
                    }
                </tr>
            }
        </tbody>
    </table>
</div>


